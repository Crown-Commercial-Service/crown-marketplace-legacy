<div class="govuk-grid-row">
  <div class="govuk-grid-column-three-quarters">
    <% if @lot.number == '4' %>
      <%= govuk_table(
        *rates_table_headers_and_rows_lot_4(@supplier_framework_lot_rates),
        first_cell_is_header: true,
        classes: 'ccs-rates-table'
      ) %>
    <% else %>
      <%= govuk_table(
        *rates_table_headers_and_rows(@supplier_framework_lot_rates),
        first_cell_is_header: true,
        classes: 'ccs-rates-table'
      ) %>
    <% end %>
  </div>
</div>
